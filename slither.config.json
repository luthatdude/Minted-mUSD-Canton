{
  "detectors_to_exclude": [
    "naming-convention",
    "pragma",
    "solc-version",
    "missing-inheritance",
    "low-level-calls",
    "unindexed-event-address",
    "unused-state",
    "too-many-digits",
    "immutable-states",
    "dead-code",
    "costly-loop",
    "calls-loop",
    "reentrancy-benign",
    "reentrancy-events",
    "timestamp",
    "locked-ether",
    "cache-array-length",
    "cyclomatic-complexity",
    "unused-return",
    "reentrancy-no-eth",
    "divide-before-multiply",
    "incorrect-equality",
    "uninitialized-local"
  ],
  "filter_paths": "node_modules|contracts/mocks|certora/harness",
  "exclude_dependencies": true,
  "exclude_informational": true,
  "exclude_low": true,
  "exclude_optimization": true
}
